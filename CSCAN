#include<bits/stdc++.h>
using namespace std;
int main(){

    int n;
    cin>>n;

    int request[n+1];
    for(int i=0 ; i<n ; i++){
        cin>>request[i];
    }

    int head;
    cin>>head;

    //assume last value
    int last_value = 199;

    sort(request , request + n);
    vector<int>ans;

    for(int i=0 ; i<n ; i++){
        if(request[i] > head){
            ans.push_back(request[i]);
        }
    }

    ans.push_back(last_value);

    ans.push_back(0);

    for(int i=0 ; i<n ; i++){
        if(request[i] < head){
            ans.push_back(request[i]);
        }
    }

    cout<<"seeksequence is"<<endl;

    for(int i=0 ; i<ans.size() ; i++){
        cout<<ans[i]<<" ";
    }

    int seekcount = 0;


    seekcount += abs(head - ans[0]);


    for(int i=1 ; i<ans.size() ; i++){
        seekcount += abs(ans[i] - ans[i-1]);
    }

    cout<<endl;

    cout<<"seekcount is"<<endl;
    cout<<seekcount<<endl;

}
